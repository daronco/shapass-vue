{
  "name": "shapass-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "favicon": "convert -density 384 -background transparent src/assets/shap-ico-dark.svg -define icon:auto-resize -colors 256 public/favicon.ico",
    "deploy": "npm run build && rsync -ru --progress dist/* daronco@shapass.com:shapass/",
    "dev-deploy": "npm run build && rsync -ru --progress dist/* daronco@shapass.com:dev.shapass/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.0",
    "vue-js-modal": "^1.3.31",
    "vue-meta": "^2.3.1",
    "vue-mobile-detection": "^1.0.0",
    "vue-router": "^3.1.2",
    "vue-shortkey": "^3.1.7",
    "vue-toasted": "^1.1.27",
    "vuejs-logger": "1.5.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-select": "^3.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
