{
  "name": "shapass-vue",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "favicon": "convert -density 384 -background transparent src/assets/shap-ico-dark.svg -define icon:auto-resize -colors 256 public/favicon.ico",
    "deploy": "vue-cli-service build --mode production && rsync -ru --progress dist/* daronco@shapass.com:shapass/",
    "dev-deploy": "vue-cli-service build --mode staging && rsync -ru --progress dist/* daronco@shapass.com:dev.shapass/",
    "test-deploy": "vue-cli-service build --mode staging && rsync -ru --progress dist/* root@test.shapass.com:/var/www/shapass/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "axios": "^0.19.2",
    "core-js": "^2.6.11",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-js-modal": "^1.3.35",
    "vue-meta": "^2.4.0",
    "vue-mobile-detection": "^1.0.0",
    "vue-router": "^3.4.8",
    "vue-shortkey": "^3.1.7",
    "vue-toasted": "^1.1.28",
    "vuejs-logger": "^1.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "vue-select": "^3.10.8",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
